Ice.MessageSizeMax=1000000000
#
# The client reads this property to create the reference to the
# "RTAReceiver" object in the server.
#
RTAReceiver.Proxy=receiver:default -p 10000 -h localhost


RTAMonitor.Proxy=monitor:default -p 20000 -h localhost

#
# Disable client-side ACM.
#
Ice.ACM.Client=0

##
# IceMX configuration.
#
#Ice.Admin.Endpoints=tcp -h localhost -p 10003
Ice.Admin.InstanceName=client
IceMX.Metrics.Debug.GroupBy=id
IceMX.Metrics.ByParent.GroupBy=parent
